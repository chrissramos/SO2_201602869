#!/usr/bin/stap

probe syscall.read {
    if(pid() == $1 || pid() == $2){
        printf("Proceso %d: Llamada realizada: READ, (%d)\n", pid(), gettimeofday_us())
    }
}

probe syscall.write {
    if(pid() == $1 || pid() == $2){
        printf("Proceso %d: Llamada realizada: WRITE , (%d)\n", pid(), gettimeofday_us())
    }
}
probe syscall.lseek {
    if(pid() == $1 || pid() == $2){
        printf("Proceso %d: Llamada realizada: SEEK, (%d)\n", pid(), gettimeofday_us())
    }
}